<ietf-client-configurations xmlns="urn:ietf:params:xml:ns:yang:ietf-tls-client">
<id>SA-ID</id>
<ietf-tls-client xmlns="urn:ietf:params:xml:ns:yang:ietf-tls-client">
<connection-info>
	<server-port>666</server-port>
	<server-address>SERVER-ADDRESS</server-address>
	<auto-start>LAST-NSF-FLAG</auto-start>
</connection-info>
<client-identity xmlns="urn:ietf:params:xml:ns:yang:ietf-tls-client">
<certificate>
	<algorithm>ks:rsa1024</algorithm>
	<certificates><certificate>
			<name>clientcertificate</name>
			<value>CLIENT-CERT</value></certificate>
	</certificates>
</certificate>
</client-identity>
<server-auth xmlns="urn:ietf:params:xml:ns:yang:ietf-tls-client">
	<pinned-ca-certs>
		<certificates>
			<certificate>
				<name>cacertificate</name>
				<value>CA-CERTIFICATE</value>
			</certificate>
		</certificates>
	</pinned-ca-certs>
</server-auth>
<hello-params xmlns="urn:ietf:params:xml:ns:yang:ietf-tls-client">
	<tls-versions>
		<tls-version>tlscmn:tls-1.3</tls-version>
		<tls-version>tlscmn:tls-1.1</tls-version>
	</tls-versions>
	<cipher-suites>
		<cipher-suite>tlscmn:ecdhe-rsa-with-aes-256-gcm-sha384</cipher-suite>
		<cipher-suite>tlscmn:ecdhe-rsa-with-aes-128-gcm-sha256</cipher-suite>
	</cipher-suites>
</hello-params>
</ietf-tls-client>
</ietf-client-configurations>
